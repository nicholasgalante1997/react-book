  {\n    id: 3,\n    title: \"Hello Javascript\",\n    content: \"Lorem ipsum\",\n    slug: 'hello-js'\n  }\n]\n\nfunction App(props) {\n\n  const [posts, setPosts] = useState(POSTS)\n  const [message, setMessage] = useState(null)\n\n  const setFlashMessage = (message) => {\n    setMessage(message)\n    setTimeout(() => {\n      setMessage(null);\n    }, 1600)\n  }\n\n  const getNewSlugFromTitle = (title) => {\n   return encodeURIComponent(\n      title.toLowerCase().split(\" \").join(\"-\")\n    )\n  }\n\n  const updatePost = (post) => {\n    post.slug = getNewSlugFromTitle(post.title)\n    const index = posts.findIndex(p => p.id === post.id)\n    const oldPosts = posts.slice(0, index).concat(posts.slice(index + 1))\n    const updatedPosts = [...oldPosts, post].sort((a, b) => a.id - b.id )\n    setPosts(updatedPosts)\n    setFlashMessage('updated')\n  }\n\n  const addNewPost = (post) => {\n    post.id = posts.length + 1\n    post.slug = getNewSlugFromTitle(post.title)\n    setPosts([...posts, post])\n    setFlashMessage('saved')\n  }\n\n  return (\n    <Router>\n      <div className=\"App\">\n      <Header />\n      {message && <Message type={message} /> }\n      <Switch>\n        <Route \n          exact path=\"/\"\n          render={ () => <Posts posts={posts} /> }\n        /> \n        <Route \n          path=\"/posts/:postSlug\"\n          render={(props) => {\n            const post = posts.find(post => post.slug === props.match.params.postSlug);\n            if (post) {\n              return <Post post={post} />\n            } else {\n              return <NotFound />\n            }\n          }}\n        />\n        <Route \n          exact path=\"/new\"\n          render={() => {\n            return <PostForm \n              addNewPost={addNewPost} \n              post={{id: 0, slug: \"\", title: \"\", content: \"\"}}\n            />\n          }}\n        />\n        <Route  \n          path=\"/edit/:postSlug\"\n          render={(props) => {\n            const post = posts.find(post => post.slug === props.match.params.postSlug)\n            if (post) {\n              return <PostForm post={post} updatePost={updatePost} />\n            } else {\n              return <Redirect to=\"/\" />\n            }\n          }}\n        />\n        <Route \n          component={NotFound}\n        />\n      </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n",["46","47"],"/Users/nicholasgalante/Code/React-Explained-ZGordon/react-book/project-working/react-explained/src/components/Posts.js",[],"/Users/nicholasgalante/Code/React-Explained-ZGordon/react-book/project-working/react-explained/src/reportWebVitals.js",[],["48","49"],"/Users/nicholasgalante/Code/React-Explained-ZGordon/react-book/project-working/react-explained/src/components/Post.js",[],"/Users/nicholasgalante/Code/React-Explained-ZGordon/react-book/project-working/react-explained/src/components/NotFound.js",[],["50","51"],"/Users/nicholasgalante/Code/React-Explained-ZGordon/react-book/project-working/react-explained/src/components/PostForm.js",[],"/Users/nicholasgalante/Code/React-Explained-ZGordon/react-book/project-working/react-explained/src/components/Message.js",[],{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","severity":1,"message":"57","line":1,"column":8,"nodeType":"58","messageId":"59","endLine":1,"endColumn":12},{"ruleId":"52","replacedBy":"60"},{"ruleId":"54","replacedBy":"61"},{"ruleId":"52","replacedBy":"62"},{"ruleId":"54","replacedBy":"63"},{"ruleId":"52","replacedBy":"64"},{"ruleId":"54","replacedBy":"65"},"no-native-reassign",["66"],"no-negated-in-lhs",["67"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar",["66"],["67"],["66"],["67"],["66"],["67"],"no-global-assign","no-unsafe-negation"]